{
	"name": "yt-backup",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"main": "dist/main/index.cjs",
	"author": "sv.gmyria@gmail.com",
	"license": "MIT",
	"scripts": {
		"dev": "concurrently -k -n VITE,TSUP,ELECTRON -c green,cyan,magenta \"vite\" \"cross-env WATCH=true tsup --watch\" \"wait-on tcp:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .\"",
		"build": "rimraf dist && vite build && tsup",
		"start": "electron .",
		"pack": "npm run build && electron-builder --dir",
		"dist": "npm run build && electron-builder"
	},
	"devDependencies": {
		"@types/node": "^24.5.2",
		"@types/react": "^19.1.13",
		"@types/react-dom": "^19.1.9",
		"@vitejs/plugin-react": "^5.0.3",
		"concurrently": "^9.2.1",
		"cross-env": "^10.0.0",
		"electron": "^38.1.2",
		"electron-builder": "^26.0.12",
		"tsu": "^2.12.0",
		"tsup": "^8.5.0",
		"typescript": "^5.9.2",
		"vite": "^7.1.7",
		"wait-on": "^9.0.1"
	},
	"dependencies": {
		"@chakra-ui/react": "^3.27.0",
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.1",
		"dexie": "^4.2.0",
		"framer-motion": "^12.23.18",
		"google-auth-library": "^10.3.0",
		"googleapis": "^160.0.0",
		"next-themes": "^0.4.6",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-icons": "^5.5.0",
		"react-virtualized-auto-sizer": "^1.0.26",
		"react-window": "^2.1.1"
	}
}
